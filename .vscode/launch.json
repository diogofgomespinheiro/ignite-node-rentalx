  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Docker",
      "port": 9270,
      "address": "localhost",
      "localRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "/user/app/*": "${workspaceRoot}/*"
      },
      "remoteRoot": "/usr/app"
    }
  ]
}
